local GamepassId = 41506473
local message = "Dumbasss"
local MarketPlaceService = game:GetService("MarketplaceService")
local Player = game.Players.LocalPlayer

-- Function to check if the player owns the game pass and kick them if not
local function checkAndKick()
    local success, ownsGamePass = pcall(function()
        return MarketPlaceService:UserOwnsGamePassAsync(Player.UserId, GamepassId)
    end)
    
    if not success then
        warn("Failed to check game pass ownership: " .. tostring(ownsGamePass))
        return
    end
    
    if not ownsGamePass then
        Player:Kick(message)
    end
end

-- Run the function to check the game pass and potentially kick the player
checkAndKick()
